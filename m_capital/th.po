#: m_capital/m_capital/doctype/card_activation/card_activation.py:303
msgid ""
"\n"
"\t\t\t\t<div style=\"font-family: Arial, sans-serif; max-width: 600px; margin: 0 auto;\">\n"
"\t\t\t\t\t<h2 style=\"color: #28a745;\">✓ Card Activation Successful</h2>\n"
"\t\t\t\t\t<p>Dear {0},</p>\n"
"\t\t\t\t\t<p>Your M Capital card has been successfully activated!</p>\n"
"\t\t\t\t\t<div style=\"background: #f5f5f5; padding: 20px; margin: 20px 0;\">\n"
"\t\t\t\t\t\t<p><strong>Card Number:</strong> {1}</p>\n"
"\t\t\t\t\t\t<p><strong>Activated Date:</strong> {2}</p>\n"
"\t\t\t\t\t</div>\n"
"\t\t\t\t\t<p>You can now use your card for loan transactions.</p>\n"
"\t\t\t\t\t<p style=\"color: #666;\">If you did not activate this card, please contact M Capital support immediately.</p>\n"
"\t\t\t\t\t<hr style=\"border: 1px solid #eee; margin: 30px 0;\">\n"
"\t\t\t\t\t<p style=\"color: #666; font-size: 12px;\">\n"
"\t\t\t\t\t\tM Capital Corporation<br>\n"
"\t\t\t\t\t\thttps://mcapital.co.th\n"
"\t\t\t\t\t</p>\n"
"\t\t\t\t</div>\n"
"\t\t\t"
msgstr ""
"<div style=\"font-family: Arial, sans-serif; max-width: 600px; margin: 0 auto;\">\n"
"\t<h2 style=\"color: #28a745;\">✓ การเปิดใช้งานบัตรสำเร็จ</h2>\n"
"\t<p>เรียน {0},</p>\n"
"\t<p>บัตร M Capital ของท่านได้รับการเปิดใช้งานเรียบร้อยแล้ว!</p>\n"
"\t<div style=\"background: #f5f5f5; padding: 20px; margin: 20px 0;\">\n"
"\t\t<p><strong>หมายเลขบัตร:</strong> {1}</p>\n"
"\t\t<p><strong>วันที่เปิดใช้งาน:</strong> {2}</p>\n"
"\t</div>\n"
"\t<p>ท่านสามารถใช้บัตรนี้สำหรับทำรายการสินเชื่อได้ทันที</p>\n"
"\t<p style=\"color: #666;\">หากท่านไม่ได้เป็นผู้เปิดใช้งานบัตรนี้ กรุณาติดต่อฝ่ายสนับสนุนของ M Capital ทันที</p>\n"
"\t<hr style=\"border: 1px solid #eee; margin: 30px 0;\">\n"
"\t<p style=\"color: #666; font-size: 12px;\">\n"
"\t\tบริษัท M Capital<br>\n"
"\t\thttps://mcapital.co.th\n"
"\t</p>\n"
"</div>"

